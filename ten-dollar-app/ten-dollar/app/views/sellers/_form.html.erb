<%= form_for @seller do |f| %>
  <div>


    <%= f.label :first_name %>:
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>:
    <%= f.text_field :last_name %>

    <%= f.label :phone %>:
    <%= f.text_field :phone %>

    <%= f.label :email %>:
    <%= f.text_field :email %><br><br>

    <%= f.label "User Name"%>:
    <%= f.text_field :username %>

    <%= f.label :password %> 
    <%= f.password_field :password %>

  </div>
  <div>
    <%= f.submit %> <br>
    <%= link_to "Cancel", sellers_path , {:style=>'color:red;', :class => "css_class"} %>
  </div>
<% end %>